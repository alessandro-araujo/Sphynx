{
    "name": "vendor/api",
    "description": "A simple PHP API project",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.0",
        "usmanhalalit/pixie": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Core\\": "src/Core/",
            "Router\\": "src/Routes/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
